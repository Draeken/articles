{
    "defaultSeverity": "error",
    "extends": [
      "tslint:latest",
      "rxjs-tslint"
    ],
    "jsRules": {},
    "rules": {
      "no-implicit-dependencies": [true, "dev"],
      "no-submodule-imports": [true, "rxjs"],
      "no-console": false,
      "quotemark": false,
      "interface-name": false,
      "object-literal-sort-keys": false,
      "trailing-comma":false,
      "arrow-parens": false
    },
    "rulesDirectory": []
}
